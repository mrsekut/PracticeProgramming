(* おそらく間違っている *)
RSAKey[a_, b_] := Module[{p, q, n, e, d=0, k, i, pqTimes},
	p = NextPrime[a];
	q = NextPrime[b];
	pqTimes = (p - 1)*(q - 1);
	n = p*q;
	e = RandomInteger[10000];
	While[! CoprimeQ[pqTimes, e],
		e = RandomInteger[100000];
	];

	k = ExtendedGCD[pqTimes, e][1];
	While[k != 1,
		k = ExtendedGCD[pqTimes, e*d][[1]];
		d++
	];
	Print[d];
]

(* 暗号化 *)
RSAenc[n_, e_, x_] := Module[{},
	Return[Mod[x^e, n]]
]

(* 複合 *)
RSAdec[n_, d_, y_] := Module[{},
	Return[Mod[y^d, N]]
]